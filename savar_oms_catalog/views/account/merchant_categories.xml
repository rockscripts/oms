<odoo>

        <template id="portal_my_categories" name="Mis Categorias" inherit_id="portal.portal_my_home" customize_show="True" priority="30">

            <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">

                <t t-call="portal.portal_docs_entry">
                  <t t-set="title">Mis Categorias</t>
                  <t t-set="url" t-value="'/my/categories'"/>
                  <!--<t t-set="placeholder_count" t-value="'order_count'"/>-->
                </t>

            </xpath>

        </template>

</odoo>